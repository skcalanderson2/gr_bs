<?xml version="1.0"?>
<block>
  <name>Descrambler Sync</name>
  <key>bs_ec_descrambler_sync</key>
  <category>bs</category>
  <import>import bs</import>
  <make>bs.ec_descrambler_sync($tap_mask, $preload)</make>
    <param>
		<name>Tap Mask</name>
		<key>tap_mask</key>
		<value>135168</value>
		<type>int</type>
	</param>
	<param>
		<name>Preload</name>
		<key>preload</key>
		<value>0</value>
		<type>int</type>
	</param>
	<sink>
		<name>in</name>
		<type>byte</type>
	</sink>
	<source>
		<name>out</name>
		<type>byte</type>
	</source>
</block>
